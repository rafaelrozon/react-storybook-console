{
  "name": "react-storybook-console",
  "version": "2.0.4",
  "description": "Display console messages in React Storybook",
  "main": "dist/index.js",
  "author": "Rafael Rozon",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rafaelrozon/react-storybook-console"
  },
  "keywords": [
    "react",
    "storybook",
    "addon",
    "console",
    "dev",
    "tool"
  ],
  "dependencies": {
    "raf": "^3.4.0",
    "react-inspector": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addons": "^4.0.4",
    "@storybook/react": "^4.0.4",
    "chai": "^3.5.0",
    "enzyme": "^2.9.1",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^4.3.0",
    "ignore-styles": "^5.0.1",
    "jest": "^23.1.0",
    "jsdom": "^8.3.1",
    "mocha": "^2.4.5",
    "react": "^16.4.0",
    "react-addons-test-utils": "^15.0.0",
    "react-dom": "^16.4.0",
    "sinon": "^1.17.3",
    "webpack": "^4.25.1"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "scripts": {
    "build": "rm -rf dist; node_modules/.bin/babel --plugins \"@babel/plugin-transform-runtime\" ./src --out-dir ./dist",
    "test": "node_modules/.bin/mocha --require tests/setup.js --require ignore-styles tests/*.test.js",
    "test:watch": "node_modules/.bin/mocha --require tests/setup.js --require ignore-styles tests/*.test.js --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  }
}
