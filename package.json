{
  "name": "react-storybook-console",
  "version": "1.0.2",
  "description": "Display console messages in React Storybook",
  "main": "dist/index.js",
  "author": "Rafael Rozon",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rafaelrozon/react-storybook-console"
  },
  "keywords": [
    "react",
    "storybook",
    "addon",
    "console",
    "dev",
    "tool"
  ],
  "dependencies": {
    "babel-runtime": "^6.5.0",
    "react-inspector": "^1.1.2"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.5.2",
    "babel-cli": "^6.5.0",
    "babel-core": "^6.5.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.5.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.22.0",
    "chai": "^3.5.0",
    "enzyme": "^2.2.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^4.3.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^8.3.1",
    "mocha": "^2.4.5",
    "react": "^15.0.0",
    "react-addons-test-utils": "^15.0.0",
    "react-dom": "^15.0.0",
    "sinon": "^1.17.3"
  },
  "peerDependencies": {
    "react": "^0.14.7 || ^15.0.0",
    "@kadira/storybook": "^2.5.2"
  },
  "scripts": {
    "build": "rm -rf dist; node_modules/.bin/babel --plugins \"transform-runtime\" ./src --out-dir ./dist",
    "test": "node_modules/.bin/mocha --require tests/setup.js --require ignore-styles tests/*.test.js",
    "test:watch": "node_modules/.bin/mocha --require tests/setup.js --require ignore-styles tests/*.test.js --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "storybook build-storybook"
  }
}
